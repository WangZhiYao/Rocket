<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_create_order"
    app:startDestination="@id/nav_create_order">

    <fragment
        android:id="@+id/nav_create_order"
        android:name="com.yizhenwind.rocket.feature.order.ui.create.CreateOrderFragment"
        android:label="@string/label_create_order"
        tools:layout="@layout/fragment_create_order">

        <argument
            android:name="clientId"
            android:defaultValue="0L"
            app:argType="long"
            app:nullable="false" />

        <argument
            android:name="accountId"
            android:defaultValue="0L"
            app:argType="long"
            app:nullable="false" />

        <argument
            android:name="characterId"
            android:defaultValue="0L"
            app:argType="long"
            app:nullable="false" />

    </fragment>

</navigation>