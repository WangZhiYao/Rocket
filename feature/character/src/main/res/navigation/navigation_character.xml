<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation_character"
    app:startDestination="@id/nav_create_fragment">

    <fragment
        android:id="@+id/nav_create_fragment"
        android:name="com.yizhenwind.rocket.feature.character.ui.create.CreateCharacterFragment"
        android:label="@string/label_create_character">

        <argument
            android:name="clientId"
            android:defaultValue="0L"
            app:argType="long"
            app:nullable="false" />

        <argument
            android:name="accountId"
            android:defaultValue="0L"
            app:argType="long"
            app:nullable="false" />

    </fragment>

</navigation>